{
  "name": "clickalicious/doozr-community-bundle",
  "description": "Getting started with Doozr in just seconds by using this bundle.",
  "type": "metapackage",
  "require": {
    "php": ">=5.5.0",
    "clickalicious/doozr": "~0.0.1"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "post-install-cmd": [
      "Doozr_Installer_Framework::postInstall"
    ]
  },
  "keywords": [
    "Doozr",
    "Community",
    "Bundle",
    "Tryout",
    "Test",
    "Prototyping"
  ],
  "homepage": "https://github.com/clickalicious/Doozr-community-bundle",
  "license": "BSD-3-Clause",
  "support": {
    "email": "opensource@clickalicious.de",
    "issues": "https://github.com/clickalicious/Doozr-community-bundle/issues",
    "wiki": "https://github.com/clickalicious/Doozr-community-bundle/wiki"
  },
  "config": {
    "bin-dir": "bin",
    "preferred-install": "dist"
  }
}
